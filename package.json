{
  "name": "vuejs-api",
  "version": "0.1.11",
  "description": "Api management wrapper for Vue",
  "jsnext:main": "src/index.js",
  "main": "dist/vuejs-api.js",
  "unpkg": "dist/vuejs-api.min.js",
  "module": "dist/vuejs-api.es.js",
  "directories": {
    "test": "test"
  },
  "repository": "https://github.com/xtoolkit/vuejs-api",
  "scripts": {
    "purge": "rm -rf dist/",
    "compile": "rollup -c --name VueJsApi && NODE_ENV=production rollup -c --name VueJsApi",
    "build": "npm run purge && npm run compile",
    "release": "standard-version && git push --follow-tags && npm publish",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "files": [
    "dist/",
    "src/",
    "nuxt/"
  ],
  "author": "Mehdi Hosseinzade <arshen@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "axios": "^0.19.0",
    "vue": "latest"
  },
  "devDependencies": {
    "rollup": "^2.23.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "rollup-plugin-terser": "^6.1.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "^7.11.0",
    "babel-eslint": "^10.1.0",
    "prettier": "^2.0.0",
    "eslint": "^7.4.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.11",
    "standard-version": "latest"
  }
}
